{
  "name": "splitflow",
  "version": "0.1.0",
  "description": "Splitflow",
  "type": "module",
  "bin": "dist/cli.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./devtool": {
      "import": "./dist/devtool.js",
      "require": "./dist/devtool.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "devtool": [
        "dist/devtool.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && tsup src/cli.ts",
    "build:dev": "tsc && vite build --mode development && tsup src/cli.ts",
    "preview": "vite preview"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/yargs": "^17.0.19",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "tsup": "^6.5.0"
  },
  "dependencies": {
    "yargs": "^17.6.2"
  }
}
