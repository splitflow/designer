{
    "name": "@splitflow/designer",
    "version": "2.1.0",
    "description": "Splitflow designer tool",
    "author": "team@splitflow.io",
    "homepage": "https://github.com/splitflow/designer#readme",
    "bugs": {
        "url": "https://github.com/splitflow/designer/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/splitflow/designer.git"
    },
    "type": "module",
    "files": [
        "dist/**/*.js",
        "dist/**/*.cjs",
        "dist/**/*.d.ts"
    ],
    "exports": {
        ".": {
            "import": "./dist/index.js",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        },
        "./react": {
            "import": "./dist/react.js",
            "require": "./dist/react.cjs",
            "types": "./dist/react/index.d.ts"
        },
        "./svelte": {
            "import": "./dist/svelte.js",
            "require": "./dist/svelte.cjs",
            "types": "./dist/svelte/index.d.ts"
        }
    },
    "typesVersions": {
        "*": {
            "*": [
                "dist/index.d.ts"
            ],
            "react": [
                "dist/react/index.d.ts"
            ],
            "svelte": [
                "dist/svelte/index.d.ts"
            ]
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "test": "vitest"
    },
    "devDependencies": {
        "@types/react": "^18.0.27",
        "react": "^18.2.0",
        "svelte": "^4.1.2",
        "typescript": "^4.9.3",
        "vite": "^4.0.0",
        "vite-plugin-dts": "^1.7.1",
        "vitest": "^0.28.4"
    },
    "peerDependencies": {
        "react": ">=16.3.0",
        "svelte": ">=3.57.0"
    },
    "peerDependenciesMeta": {
        "react": {
            "optional": true
        },
        "svelte": {
            "optional": true
        }
    },
    "dependencies": {
        "@splitflow/core": "^2.1.0",
        "@splitflow/lib": "^1.1.0"
    }
}
