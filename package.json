{
  "name": "@splitflow/designer",
  "version": "1.2.2",
  "description": "Splitflow designer tool",
  "author": "team@splitflow.io",
  "homepage": "https://github.com/splitflow/designer#readme",
  "bugs": {
    "url": "https://github.com/splitflow/designer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/splitflow/designer.git"
  },
  "type": "module",
  "files": [
    "dist/**.js",
    "dist/**.cjs",
    "dist/**.d.ts"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./app": {
      "import": "./dist/app.js",
      "require": "./dist/app.cjs"
    },
    "./style": {
      "import": "./dist/style.js",
      "require": "./dist/style.cjs"
    },
    "./react": {
      "import": "./dist/react.js",
      "require": "./dist/react.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/index.d.ts"
      ],
      "app": [
        "dist/app.d.ts"
      ],
      "style": [
        "dist/style.d.ts"
      ],
      "react": [
        "dist/react.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "react": "^18.2.0",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "vite-plugin-dts": "^1.7.1",
    "vitest": "^0.28.4"
  },
  "peerDependencies": {
    "react": ">=16.3.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "dependencies": {
    "core": "npm:@splitflow/core@^1.2.0"
  }
}
