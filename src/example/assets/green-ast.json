{
    "App-root": {
        "background": { "color": [150, 50, 60] },
        "layout": {
            "direction": "column",
            "mainAxisAlignment": "start",
            "crossAxisAlignment": "stretch",
            "spacing": 14
        }
    },
    "type": [0, 0, 30, 1],
    "Nav-root": {
        "layout": {
            "direction": "row",
            "mainAxisAlignment": "end",
            "crossAxisAlignment": "stretch",
            "spacing": 1
        },
        "padding": { "top": 1, "bottom": 1, "left": 1, "right": 1 }
    },
    "Nav-button": {
        "padding": { "top": "0.5", "bottom": 0.5, "left": 1, "right": 1 },
        "corner": { "topLeft": 0.75, "topRight": 0.75, "bottomLeft": 0.75, "bottomRight": 0.75 },
        "typography": { "fontSize": 1, "fontWeight": 400, "color": [0, 0, 0] }
    },
    "Nav-button:green": { "background": { "color": [150, 50, 70] } },
    "Nav-button:red": { "background": { "color": [0, 50, 70] } },
    "Nav-button:reset": { "background": { "color": [0, 0, 100] } },
    "Body-root": {
        "layout": {
            "direction": "column",
            "mainAxisAlignment": "start",
            "crossAxisAlignment": "center",
            "spacing": 1
        }
    },
    "Body-title": { "typography": { "fontSize": 4, "fontWeight": 600, "color": [0, 0, 0, 1] } },
    "Body-feature": {
        "typography": { "fontSize": 1.25, "fontWeight": 400, "color": [0, 0, 0, 1] }
    },
    "Nav-button:hover": {
        "typography": { "fontSize": 1, "fontWeight": 400, "color": [0, 0, 30, 1] }
    }
}
